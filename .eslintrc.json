{
  "env": {
    "es2021": true
  },
  "extends": [
    "standard-with-typescript",
    "plugin:prettier/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.test.json"
  },
  "plugins": [
    "prettier"
  ],
  "ignorePatterns": [
    "dist/*"
  ],
  "rules": {
    "@typescript-eslint/explicit-member-accessibility": "error"
  }
}
